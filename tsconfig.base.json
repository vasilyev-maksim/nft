{
  // "include": ["./src/*.ts"],
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitThis": true,
    // "types": ["node"],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    // "moduleResolution": "Node"
    "strict": true,
    "useDefineForClassFields": true,
    "moduleResolution": "Node"
  }
}
