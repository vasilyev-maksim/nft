{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "noEmit": false,
    "declaration": true,
    "strict": true,
    "outDir": "dist",
    "lib": ["ES2019.Array"],
    "module": "CommonJS"
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
