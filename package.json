{
  "name": "nft",
  "version": "1.0.0",
  "description": "This project runs React web client and API server of NFT generator app",
  "main": "start.js",
  "author": "Maksim Vasilyev",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/node": "^17.0.25",
    "kill-port": "^1.6.1"
  },
  "scripts": {
    "start": "kill-port 3001 3002 && node dist/start.js",
    "watch": "tsc -w start.ts",
    "build": "tsc && tsc -b -f -v",
    "clean": "npm run build && rm -rf ./api/collections/**/*snapshot* ./node_modules ./logs/* ./client/node_modules ./client/yarn*.* ./client/package-lock.json ./api/node_modules ./api/yarn*.* ./api/package-lock.json"
  }
}
